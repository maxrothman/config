		</dict>
		<dict>
			<key>config</key>
			<dict>
				<key>applescript</key>
				<string>on alfred_script(q)
	tell application "iTerm"
		tell the current session of current window
			write text "open -a Finder ./"		end tell
	end tellend alfred_script</string>
